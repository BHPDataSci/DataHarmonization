TITLE: Second Order Factor Analysis;
DATA: FILE IS data.dat;
VARIABLE: 
NAMES ARE 
GPEGDOM 
GPEGNDOM 
STRP1GV 
STRP2GV 
STRP3GV 
TRLA 
COLOR1 
COLOR2 
PASAT 
HVLTTOT 
HVLTDELR 
SEMFLU 
AFLU;
MODEL:
MEM BY HVLTTOT* HVLTDELR;
MEM(var_MEM);
EXEC BY STRP1GV* STRP2GV STRP3GV;
EXEC(var_EXEC);
SPEED BY TRLA* COLOR1 COLOR2 PASAT;
SPEED(var_SPEED);
MOT BY GPEGDOM* GPEGNDOM;
MOT(var_MOT);
VERB BY SEMFLU* AFLU;
VERB(var_VERB);
G BY 
MEM*(G_MEM)
EXEC(G_EXEC)
SPEED(G_SPEED)
MOT(G_MOT)
VERB(G_VERB);
G@1;
[G@0];
MODEL CONSTRAINT:
var_MEM = 1 - G_MEM^2;
var_EXEC = 1 - G_EXEC^2;
var_SPEED = 1 - G_SPEED^2;
var_MOT = 1 - G_MOT^2;
var_VERB = 1 - G_VERB^2;
ANALYSIS:
ESTIMATOR = MLR;
OUTPUT: STDYX RESIDUAL;
SAVEDATA:
RESULTS ARE second_order_factor_model_results.txt;
save=fscores;
file=second_order_factor_model_fscore.dat;

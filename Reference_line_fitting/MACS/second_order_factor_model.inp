TITLE: Second Order Factor Analysis;
DATA: FILE IS data.dat;
VARIABLE: 
NAMES ARE 
GPEGDOM 
GPEGNDOM 
STRP1CV 
STRP2CV 
STRP3CV 
TRLA 
TRLB 
SYDM 
RAVTOT 
RAVIR 
RAVDELR;
MODEL:
MEM BY RAVTOT* RAVIR RAVDELR;
MEM(var_MEM);
VerPS BY STRP1CV* STRP2CV STRP3CV;
VerPS(var_VerPS);
ATT_PS BY TRLA* TRLB SYDM;
ATT_PS(var_ATT_PS);
MOT BY GPEGDOM* GPEGNDOM;
MOT(var_MOT);
G BY 
MEM*(G_MEM)
VerPS(G_VerPS)
ATT_PS(G_ATT_PS)
MOT(G_MOT);
G@1;
[G@0];
MODEL CONSTRAINT:
var_MEM = 1 - G_MEM^2;
var_VerPS = 1 - G_VerPS^2;
var_ATT_PS = 1 - G_ATT_PS^2;
var_MOT = 1 - G_MOT^2;
ANALYSIS:
ESTIMATOR = MLR;
OUTPUT: STDYX RESIDUAL;
SAVEDATA:
RESULTS ARE second_order_factor_model_results.txt;
save=fscores;
file=second_order_factor_model_fscore.dat;
